{
  "name": "quick-download-photo-weibo",
  "version": "1.0.2",
    "permissions": [
    "tabs", 
    "https://weibo.com/*",
    "http://weibo.com/*",
    "activeTab",
    "webNavigation"
  ],
  "description": "one-click download for Weibo",
  "background": {
    "scripts": ["start.js"]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Go!"
  },
  "manifest_version": 2
}
